{
    "routes": [
        {
            "src": "/src/.*",
            "dest": "/404"
        },
        {
            "src": "/api/current-text",
            "dest": "/api/current-text.js"
        }
    ],
    "headers": [
        {
            "source": "/api/current-text",
            "headers": [
                { "key": "Content-Type", "value": "text/plain" }
            ]
        }
    ]
}
